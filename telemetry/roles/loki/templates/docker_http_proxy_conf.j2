[Service]
Environment="HTTP_PROXY={{ proxy[0].http_proxy | default('', true) }}"
Environment="HTTPS_PROXY={{ proxy[0].http_proxy | default('', true) }}"
Environment="NO_PROXY=localhost,{{ cp_hostname }},*.{{ cp_domain_name }},{{ cp_ip_addresses | join(',') }}{% if no_proxy_input_status %},{{ proxy[0].no_proxy }}{% endif %}"